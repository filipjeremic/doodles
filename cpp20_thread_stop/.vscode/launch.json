  "version": "0.2.0",
  "configurations": [
    {
      "name": "cpplaunch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/${fileBasenameNoExtension}",
      "externalConsole": false,
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "cppbuild"
    }
  ]
}
